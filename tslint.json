{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "jsx-no-multiline-js":[false],
    "import-name": [false],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ]
  },
  "rulesDirectory": []
}